{
  "name": "meteor-todos",
  "private": true,
  "scripts": {
    "start": "meteor run",
    "test": "meteor test --once --driver-package meteortesting:mocha --port 8378",
    "test-on": "TEST_WATCH=1 meteor test --driver-package meteortesting:mocha --port 8378",
    "test-app": "TEST_WATCH=1 meteor test --full-app --driver-package meteortesting:mocha --port 8378",
    "visualize": "meteor --production --extra-packages bundle-visualizer --port 4000"
  },
  "dependencies": {
    "@babel/runtime": "^7.8.3",
    "bcrypt": "^5.0.0",
    "classnames": "^2.2.6",
    "jquery": "^3.4.1",
    "meteor-node-stubs": "^1.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "meteor": {
    "mainModule": {
      "client": "client/main.js",
      "server": "server/main.js"
    },
    "testModule": "tests/main.js"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "prettier": "2.0.5"
  }
}
